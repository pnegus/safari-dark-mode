{
    "manifest_version": 3,
    "default_locale": "en",

    "name": "Safari Dark Mode",
    "description": "Simple always on dark mode filter",
    "version": "1.0",
    
    "permissions": [],
    "host_permissions": [
            "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["preload.css"],
            "js": ["preload.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["<all_urls>"],
            "css": ["postload.css"],
            "js": ["postload.js"],
            "run_at": "document_end"
        }
    ]
}
